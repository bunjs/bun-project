{
	"presets": [
		[
			"@babel/preset-env", {
				"targets": { // 配支持的环境

					"node": "current"
				},
				"modules": 'commonjs',
				"debug": false, // debug，编译的时候 console
				"useBuiltIns": "usage", // 是否开启自动支持 polyfill
			}
		]
	],
	plugins: [
		["import", { "libraryName": "antd", "libraryDirectory": "lib", "style": "css" }],
		"transform-react-jsx"
	]
}
